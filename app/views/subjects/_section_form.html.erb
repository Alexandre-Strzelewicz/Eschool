<div class="section-each">
  <div class="clearfix">
    <%= s.label :enabled %>
    <div class="input">
      <%= s.check_box :enabled %>
    </div>
  </div>
    
  <div class="clearfix">
    <%= s.label :section_title %>
    <div class="input">
      <%= s.text_field :section_title %>
    </div>
  </div>

  <div class="clearfix">
    <%= s.label :section_number %>
    <div class="input">
      <%= s.text_field :section_number %>
    </div>
  </div>

  <div class="clearfix">
    <%= s.label :resume %>
    <div class="input">
      <%= s.text_field :resume %>
    </div>
  </div>

  <div class="clearfix">
    <%= s.label :video %>
    <div class="input">
      <% if s.object.new_record? %>
	lol
      <% end %>
      <%= s.file_field :video %>
      
    </div>
  </div>


</div>
